@page
@using TorneoFutbol.App.Dominio
@model TorneoFutbol.App.Frontend.Pages.Municipios.EditModel
@{
}
<h1>Actualizar Municipio</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="municipio.Id"></label>
        <input asp-for="municipio.Id" class="form-control" readonly>
    </div> 
    <div class="form-group">
        <label asp-for="municipio.Nombre"></label>
        <input asp-for="municipio.Nombre" class="form-control" placeholder="Ingrese el Nombre del Municipio">
        <span class="text-danger" asp-validation-for="municipio.Nombre"></span>
    </div>  
    <button type="submit" class="btn btn-primary btn-block">Actualizar Municipio</button> 
</form>
<br><br><a class="btn btn-secondary" asp-page="./Index">Volver</a>